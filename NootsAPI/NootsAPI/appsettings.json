{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Rabbit": "amqp://guest:guest@localhost:5672",
  "Mongo": {
    "client": "mongodb://localhost:27017",
    "database": "noots"
  },
  "FirebaseOptions": {
    "Issuer": "https://securetoken.google.com/noots-storm",
    "Audience": "noots-storm",
    "Authority": "https://securetoken.google.com/noots-storm"
  },
  "elasticsearch": {
    "index": "noots_index",
    "url": "http://localhost:9200/"
  },
  "AllowedHosts": "*"
}
